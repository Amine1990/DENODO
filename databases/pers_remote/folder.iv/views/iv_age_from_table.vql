CREATE OR REPLACE VIEW iv_age_from_table FOLDER = '/iv' AS SELECT "AGE"."ID" AS "ID", "AGE"."AGE" AS "AGE", "FIRSTTABLE"."NAME" AS "NAME" FROM "AGE" AS "AGE" INNER JOIN "FIRSTTABLE" AS "FIRSTTABLE" ON "AGE"."ID" = "FIRSTTABLE"."ID" ;

ALTER VIEW iv_age_from_table
 LAYOUT ("AGE" = [342, 218, 200, 200], "FIRSTTABLE" = [785, 217, 200, 200]);

